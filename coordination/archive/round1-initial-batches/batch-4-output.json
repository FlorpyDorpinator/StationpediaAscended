[
  {
    "deviceKey": "ThingStructureDaylightSensor",
    "displayName": "Daylight Sensor",
    "logicDescriptions": {
      "Mode": {
        "dataType": "Integer",
        "range": "0-2",
        "description": "0 = Default, 1 = Horizontal angle, 2 = Vertical angle."
      },
      "Activate": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 when sensor has sunlight, 0 when dark."
      },
      "Horizontal": {
        "dataType": "Float",
        "range": "0-360°",
        "description": "Horizontal solar angle in degrees."
      },
      "Vertical": {
        "dataType": "Float",
        "range": "-90 to 90°",
        "description": "Vertical solar angle in degrees."
      },
      "SolarAngle": {
        "dataType": "Float",
        "range": "0-180°",
        "description": "Angle between sensor forward direction and sun, in degrees. Affected by Mode setting."
      },
      "On": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Enable/disable the sensor."
      },
      "SolarIrradiance": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Current solar irradiance in watts. 0 when no sunlight."
      }
    }
  },
