  {
    "deviceKey": "ThingStructureBatteryMedium",
    "displayName": "Battery (Medium)",
    "logicDescriptions": {
      "Power": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 if battery has stored power above 0, 0 if empty."
      },
      "Mode": {
        "dataType": "Integer",
        "range": "0-6",
        "description": "Charge level: 0 = Empty, 1 = Critical, 2 = VeryLow, 3 = Low, 4 = Medium, 5 = High, 6 = Full."
      },
      "Charge": {
        "dataType": "Float",
        "range": "0+ J",
        "description": "Current stored energy in joules."
      },
      "Maximum": {
        "dataType": "Float",
        "range": "J",
        "description": "Maximum energy storage capacity in joules."
      },
      "Ratio": {
        "dataType": "Float",
        "range": "0-1",
        "description": "Charge level as ratio (Charge / Maximum)."
      },
      "PowerPotential": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Potential load on input network."
      },
      "PowerActual": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Actual current load on input network."
      },
      "On": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Enable/disable battery charging and discharging."
      }
    }
  },
