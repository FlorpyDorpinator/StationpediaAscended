[
  {
    "deviceKey": "ThingStructurePoweredVent",
    "displayName": "Powered Vent",
    "logicDescriptions": {
      "On": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Enable/disable the vent. When on and powered, actively exchanges atmosphere."
      },
      "Mode": {
        "dataType": "Integer",
        "range": "0-1",
        "description": "0 = Inward (pull gas from world into pipe), 1 = Outward (push gas from pipe to world)."
      },
      "PressureExternal": {
        "dataType": "Float",
        "range": "0-60795 kPa",
        "description": "Target pressure for the external (world) side. Vent stops when pressure reaches this value."
      },
      "Power": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Current power consumption in watts."
      },
      "Error": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 if device has an error, 0 otherwise."
      },
      "Lock": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 if device is locked, preventing manual interaction."
      },
      "RequiredPower": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Power required for device operation (250W)."
      }
    }
  },
