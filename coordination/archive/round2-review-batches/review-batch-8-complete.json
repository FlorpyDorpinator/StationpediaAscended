[
  {
    "deviceKey": "ThingStructureSorter",
    "displayName": "Sorter",
    "logicDescriptions": {
      "Power": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Returns 1 if device is powered."
      },
      "Mode": {
        "dataType": "Integer",
        "range": "0-2",
        "description": "Sorting mode: 0 = Split (alternates outputs), 1 = Filter (uses filter list), 2 = Logic (controlled by Output value)."
      },
      "Error": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 if device has an error, 0 otherwise."
      },
      "Lock": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "1 if device is locked, preventing manual interaction."
      },
      "On": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Device on/off state."
      },
      "RequiredPower": {
        "dataType": "Float",
        "range": "0+ W",
        "description": "Power draw in watts (5W)."
      },
      "ClearMemory": {
        "dataType": "Boolean",
        "range": "0-1",
        "description": "Write 1 to clear filter memory."
      },
      "ExportCount": {
        "dataType": "Integer",
        "range": "0+",
        "description": "Number of items in main export slot."
      },
      "ImportCount": {
        "dataType": "Integer",
        "range": "0+",
        "description": "Number of items in import slot."
      },
      "Output": {
        "dataType": "Integer",
        "range": "0-1",
        "description": "In Logic mode, write 0 to output straight, 1 to output right. Read returns current output selection."
      },
      "PrefabHash": {
        "dataType": "Hash",
        "range": "CRC32",
        "description": "Device type hash."
      },
      "ReferenceId": {
        "dataType": "Hash",
        "range": "Unique",
        "description": "Unique reference ID."
      },
      "NameHash": {
        "dataType": "Hash",
        "range": "CRC32",
        "description": "Custom name hash."
      }
    },
    "slotDescriptions": {
      "Import": {
        "slotType": "Import",
        "description": "Input slot for items to be sorted."
      },
      "Export": {
        "slotType": "Export",
        "description": "Main output (straight through)."
      },
      "Export 2": {
        "slotType": "Export 2",
        "description": "Secondary output (right side)."
      },
      "Data Disk": {
        "slotType": "Data Disk",
        "description": "Data disk slot for filter configuration."
      }
    }
  },
